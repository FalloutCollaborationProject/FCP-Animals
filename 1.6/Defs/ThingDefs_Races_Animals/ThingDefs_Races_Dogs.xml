    <!--___ Vicious Dog ___-->

    <!--__ ThingDefs __-->

    <!-- Vicious Dog Base -->

    <ThingDef Abstract="True" ParentName="AnimalThingBase" Name="FCP_Animal_Vicious_Dog_Base">
        <statBases>
            <MoveSpeed>5.0</MoveSpeed>
            <MarketValue>350</MarketValue>
            <ComfyTemperatureMin>-30</ComfyTemperatureMin>
            <ToxicEnvironmentResistance>0.90</ToxicEnvironmentResistance>
        </statBases>
        <tools>
            <li>
                <label>left claw</label>
                <labelNoLocation>claw</labelNoLocation>
                <capacities>
                    <li>Scratch</li>
                </capacities>
                <power>9.0</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
            </li>
            <li>
                <label>right claw</label>
                <labelNoLocation>claw</labelNoLocation>
                <capacities>
                    <li>Scratch</li>
                </capacities>
                <power>9.0</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
            </li>
            <li>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>15.0</power>
                <cooldownTime>2.6</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                <chanceFactor>0.7</chanceFactor>
            </li>
            <li>
                <label>head</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>6</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <race>
            <animalType>Canine</animalType>
            <body>QuadrupedAnimalWithPawsAndTail</body>
            <baseBodySize>0.75</baseBodySize>
            <baseHungerRate>0.4</baseHungerRate>
            <baseHealthScale>1.0</baseHealthScale>
            <foodType>OmnivoreAnimal, OvivoreAnimal</foodType>
            <leatherDef>Leather_Dog</leatherDef>
            <gestationPeriodDays>10</gestationPeriodDays>
            <nameOnTameChance>1</nameOnTameChance>
            <trainability>Advanced</trainability>

            <nuzzleMtbHours>12</nuzzleMtbHours>
            <soundEating>PredatorSmall_Eat</soundEating>
            <wildBiomes>
                <TemperateForest>0.4</TemperateForest>
                <AridShrubland>0.8</AridShrubland>
                <Desert>0.3</Desert>
                <ExtremeDesert>0.1</ExtremeDesert>
                <TemperateSwamp>0.3</TemperateSwamp>
            </wildBiomes>
            <litterSizeCurve>
                <points>
                    <li>(0.5, 0)</li>
                    <li>(1, 1)</li>
                    <li>(2, 1.7)</li>
                    <li>(3, 0.7)</li>
                    <li>(3.5, 0)</li>
                </points>
            </litterSizeCurve>
            <lifeExpectancy>12</lifeExpectancy>
            <lifeStageAges>
                <li>
                    <def>AnimalBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>AnimalJuvenile</def>
                    <minAge>0.25</minAge>
                </li>
                <li>
                    <def>AnimalAdult</def>
                    <minAge>0.5</minAge>
                    <soundWounded>FCP_Sound_Animal_Vicious_Dog_Injured</soundWounded>
                    <soundDeath>FCP_Sound_Animal_Vicious_Dog_Death</soundDeath>
                    <soundCall>FCP_Sound_Animal_Vicious_Dog_Bark</soundCall>
                    <soundAngry>FCP_Sound_Animal_Vicious_Dog_Growl</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
            <headPosPerRotation>
                <li>(0.0, 0, 0.24)</li>
                <li>(0.28, 0, 0.14)</li>
                <li>(0.0, 0, -0.03)</li>
                <li>(-0.28, 0, 0.14)</li>
            </headPosPerRotation>
        </race>
    </ThingDef>

    <!-- Vicious Dog -->

    <ThingDef ParentName="FCP_Animal_Vicious_Dog_Base">
        <defName>FCP_Animal_Vicious_Dog</defName>
        <label>Vicious Dog</label>
        <description>Vicious dogs are a nearly hairless and apparently, partially ghoulified species of wasteland canine. Ruthless survivors of the wastes, these starving, irradiated mutts will attack anything that moves.</description>
        <race>
            <wildBiomes>
                <TemperateForest>0.4</TemperateForest>
                <AridShrubland>0.8</AridShrubland>
                <Desert>0.3</Desert>
                <ExtremeDesert>0.1</ExtremeDesert>
                <TemperateSwamp>0.3</TemperateSwamp>
            </wildBiomes>
        </race>
        <tradeTags>
            <li>FCP_Animal_Dog</li>
        </tradeTags>
    </ThingDef>

    <!-- Guard Dog -->

    <ThingDef ParentName="FCP_Animal_Vicious_Dog_Base">
        <defName>FCP_Animal_Vicious_Guard_Dog</defName>
        <label>Vicious Dog</label>
        <description>Vicious dogs are a nearly hairless and apparently, partially ghoulified species of wasteland canine. Ruthless survivors of the wastes, these starving, irradiated mutts will attack anything that moves. Some dogs have been domesticated, even charged with guarding its owner and all their property this animal will do so to the best of its ability.</description>
        <race>
            <petness>1</petness>
            <wildBiomes Inherit="False" />
        </race>
        <tradeTags>
            <li>FCP_Animal_Guard_Dog</li>
        </tradeTags>
    </ThingDef>

