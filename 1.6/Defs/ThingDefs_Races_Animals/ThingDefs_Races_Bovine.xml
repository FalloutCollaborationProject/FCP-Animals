    <!-- Bighorner -->

    <ThingDef ParentName="FCP_Animal_Bighorner_Base">
        <defName>FCP_Animal_Bighorner</defName>
        <label>bighorner</label>
        <description>A mutated descendant of bighorn sheep. These hardy creatures are commonly used as pack animals and livestock throughout the wasteland, valued for their strength, meat, and hide. Males sport impressive curved horns and are larger and more aggressive than females.</description>
        <race>
            <hasGenders>true</hasGenders>
        </race>
        <tradeTags>
            <li>FCP_Animal_Bighorner</li>
        </tradeTags>
    </ThingDef>

