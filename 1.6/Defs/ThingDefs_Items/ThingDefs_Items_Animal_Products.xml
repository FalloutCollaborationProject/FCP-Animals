    <!--_____ Eggs _____-->

    <!--___ Deathclaw Egg ___-->

    <!-- Unfertilized -->

    <ThingDef ParentName="EggUnfertBase">
        <defName>FCP_Egg_Deathclaw_Unfertilized</defName>
        <label>Deathclaw Egg (unfert.)</label>
        <description>An unfertilized deathclaw egg. These massive eggs are incredibly rare and valuable. While they can be eaten, most would consider it a waste of such a valuable commodity.</description>
        <graphicData>
            <texPath>Things/Items/Animal_Products/Eggs/Deathclaw/FCP_Deathclaw_Egg</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <drawSize>1.2</drawSize>
        </graphicData>
        <stackLimit>10</stackLimit>
        <statBases>
            <MarketValue>150.0</MarketValue>
            <Nutrition>2.5</Nutrition>
            <FoodPoisonChanceFixedHuman>0.25</FoodPoisonChanceFixedHuman>
        </statBases>
    </ThingDef>

    <!-- Fertilized -->

    <ThingDef ParentName="EggFertBase">
        <defName>FCP_Egg_Deathclaw_Fertilized</defName>
        <label>Deathclaw Egg (fert.)</label>
        <description>A fertilized deathclaw egg. If properly incubated, this will hatch into one of the most dangerous predators in the wasteland. These eggs are extremely rare and highly sought after by beast masters and scientists alike. It can be eaten, but most would consider that a terrible waste.</description>
        <possessionCount>1</possessionCount>
        <graphicData>
            <texPath>Things/Items/Animal_Products/Eggs/Deathclaw/FCP_Deathclaw_Egg</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <drawSize>1.2</drawSize>
        </graphicData>
        <stackLimit>10</stackLimit>
        <statBases>
            <MarketValue>300.0</MarketValue>
            <Nutrition>2.5</Nutrition>
            <FoodPoisonChanceFixedHuman>0.25</FoodPoisonChanceFixedHuman>
        </statBases>
        <comps>
            <li Class="FCP.Core.CompProperties_HatcherCustom">
                <hatcherDaystoHatch>25</hatcherDaystoHatch>
                <hatcherPawn>FCP_Animal_Deathclaw</hatcherPawn>
                <secondaryOverrideChance>0.05</secondaryOverrideChance>
                <secondaryPawn>FCP_Animal_Alpha_Deathclaw</secondaryPawn>
            </li>
        </comps>
    </ThingDef>

